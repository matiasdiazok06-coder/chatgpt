# Valores por defecto para los parámetros de envío y auto-respuesta
# Podés crear un `.env.local` para sobrescribirlos sin tocar este archivo.
MAX_PER_ACCOUNT=10
MAX_CONCURRENT=5
DELAY_MIN=5
DELAY_MAX=15
AUTORESPONDER_DELAY=10

# Logging y modo silencioso
QUIET=0
LOG_DIR=storage/logs
LOG_FILE=app.log

# Clave de OpenAI (opcional si usás auto-responder)
OPENAI_API_KEY=
